{
  "manifest_version": 2,

  "name": "DisConnect2",
  "description": "This extension fixes URLs broken by the EA vpn",
  "version": "1.0",

  "icons": { "16": "icon.png",
             "48": "icon48.png",
            "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Paste URL to fix"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}